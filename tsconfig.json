{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"lib": ["es2020", "dom", "dom.iterable"],
		"module": "ESNext",
		"moduleResolution": "bundler",
		"noEmit": false,
		"outDir": "lib",
		"strict": true,
		"target": "esnext"
	},
	"include": ["./src"],
	"exclude": []
}